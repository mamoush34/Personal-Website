{
    "name": "personal-website",
    "version": "2.0.0",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "description": "Mohammad Amoush's Personal Portfolio Website",
    "main": "index.ts",
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 ts-node-dev -- src/server/index.ts",
        "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 ts-node-dev -- src/server/index.ts",
        "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --mode production",
        "build:dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --mode development",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,scss,css}\""
    },
    "repository": {
        "type": "git",
        "url": "git+"
    },
    "author": "Mohammad Amoush",
    "license": "ISC",
